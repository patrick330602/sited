    <jscript>
		<code>
        <![CDATA[


function bookUrl(url){
	if(url.indexOf("http")>=0)
		return url;
	else
		return "http://www.jmydm.com" + url;
}

function hots_parse(url,html) {
	var list = [];
    var reg  = /<a.[^\/]*([^']*)[^>]*><img.alt=.([^']*)..src=.([^>']*)/gi;

	var rst;
	while((rst = reg.exec(html))!=null){
		var bm = {};
		bm.name= rst[2];
		bm.url = bookUrl(rst[1]);
		bm.logo = rst[3];

		list.push(bm);
	}
    return JSON.stringify(list);
}

function updates_parse(url,html) {
    var list = [];
    var reg  = /<a.[^\/]*([^']*)[^>]*>([^>]*)<span([^>]*>){7}(\d+-\d+)([^>]*>){1}([^<]*)/gi;

	var rst;
	while((rst = reg.exec(html))!=null){
		var bm = {};
		bm.name= rst[2];
		bm.url = bookUrl(rst[1]);
		bm.updateTime = rst[4];
		bm.newSection = rst[6];

		list.push(bm);
	}
    return JSON.stringify(list);
}

function search_parse(url,html) {
    var list = [];

	html = html.replace(/<\/?font[^>]*>/gi,"");

	var reg = /cInfoItem.><div[^>]*><img.alt=.[^']*..src=.([^']*).>([^>]*>){2}<a.target=._blank..href=.([^']*).>([^<]*)<span.class=.clw\d+.>([^<]*)<\/span><\/a>(<span.class=.ctor.>[^<*]<\/span>)?([^>]*>){14}/gi;

	var rst;
	while((rst = reg.exec(html))!=null){
		var bm = {};
		bm.name = rst[4].replace(/<\/?font[^>]*>/gi,"");
		bm.url = bookUrl(rst[3]);
		bm.logo = rst[1];
		bm.newSection = rst[5];
		bm.updateTime = rst[0].match(/(\d+-\d+-\d+)/)[1];
		bm.status = "";
		list.push(bm);
	}
	return JSON.stringify(list);
}

function tags_parse(url,html) {
	var list = [];
	var reg = /<div>\s*<dd[^>]*><a.target=._blank..href=.([^']*).[^>]*>\s*<img.alt=.([^']*)..src=.([^']*).>([^>]*>){4}([^<]*)/gi;

	var rst;
	while((rst = reg.exec(html))!=null){
		var bm = {};
		bm.name = rst[2];
		bm.url = bookUrl(rst[1]);
		bm.logo = rst[3];
		bm.newSection = rst[5].trim();
		bm.updateTime = "";
		bm.status = "";

		list.push(bm);
	}
	return JSON.stringify(list);
}

function book_parse(url,html) {
	var data={};

	data.name =   html.match(/<td>\s*<b>\s*([^<]*)/i)[1];
	data.author = html.match(/author\/goauthor[^>]*>([^<]*)/i)[1];
	data.logo = html.match(/cDefaultImg.>\s*<img.src=.([^']*)./i)[1];
	data.updateTime="";
	data.intro  = html.match(/cInfoArea.>\s*([^<]*)/i)[1].trim();
	data.sections=[];

	var reg = /<li>([^>]*>){4}<a\s*(class=.[^']*.)?\s*target=._blank..href=.([^']*).>([^<]*)/gi;
	var rst;
	while((rst = reg.exec(html))!=null){
		var sm = {url:bookUrl(rst[3]),name:rst[4]};

		data.sections.push(sm);
	}
	return JSON.stringify(data);
}

function book_url(url) {
	return url;
}

function book_key(url) {
	return "jmydm" + url.match(/([^\/]*)\/$/i)[1];
}

function section_parse(url,html) {
	var list = [];

	var urlsStr = html.match(/lf\d+=.([^\"]*)./i)[1];

	if(urlsStr.indexOf("|")<0){
		urlsStr = unsuan(urlsStr,"kxnelimwzsb");
	}

	if(urlsStr.indexOf("|")>=0){
		var path = "http://comic.jmydm.com:8080/"+ html.match(/sPath=.([^\"]*)./)[1];
        var urls = urlsStr.split("|");

		for(var i=0,len=urls.length; i<len; i++){
			var u1 = unescape(urls[i]);
			list.push(path + u1);
		}
	}

	return JSON.stringify(list);
}

function section_key(url) {
	return url;
}


function unsuan(s,sk)
{
	var k=sk.substring(0,sk.length-1);
	var f=sk.substring(sk.length-1);
	for(var i=0;i<k.length;i++) {
	    eval("s=s.replace(/"+ k.substring(i,i+1) +"/g,'"+ i +"')");
	}
    var ss = s.split(f);
	s="";
	for(var i=0;i<ss.length;i++) {
	    s+=String.fromCharCode(ss[i]);
    }
    return s;
}

    ]]>
